#include "sort.h"
#include <stdio.h>

/**
 * insertion_sort_list - insertion sort alg
 * @list: list
 * Return: no return
 */
void insertion_sort_list(listint_t **list)
{
listint_t *tmp = NULL, *tmp1 = NULL;
int f;
if (!list && !*list)
return;
if (!(*list)->next)
return;
tmp = (*list)->next;
while (tmp)
{
f = 0;
while (tmp->prev && tmp->prev->n > tmp->n)
{
f = 1;
tmp1 = tmp->next;
tmp->prev->next = tmp1;
tmp->next = tmp->prev;
if (tmp->next)
tmp->next->prev = tmp->prev;
tmp->prev = tmp->prev->prev;
tmp->prev->prev->next = tmp;
print_list(*list);
if (f == 1)
break;
}
tmp = tmp->next;
}
}
